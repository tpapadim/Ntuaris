                        WHERE ( YPCOURSE.KAT = :3  OR YPCOURSE.KAT = '00' )  AND
		       YPCOURSE.AK = :4  AND OMADA IS NOT NULL AND EJ = :5 AND
                                       YPCOURSE.K_TM = :1 AND YPCOURSE.K_M = :2
UNION
                        SELECT DISTINCT omada,  'YX', EJ
                        FROM  YPCOURSE
                        WHERE  OMADA IS NULL AND
                           	( ypcourse.kat = :3 OR ypcourse.kat = '00' )  AND  ypcourse.ak = :4  AND
		  ypcourse.k_tm = :1 AND ypcourse.K_M = :2 AND EJ = :5
UNION
                         SELECT DISTINCT omada , 'GL', EJ
                          FROM LANG
                          WHERE lang.ak =:4  AND LANG.K_M = :2 AND EJ = :5
UNION
                         SELECT DISTINCT NULL,  'PR' , EJ
                          FROM  PROAIRET
                          WHERE  proairet.ak = :4  AND
                                        (proairet.kat = :3  OR proairet.KAT = '00')  AND
		      PROAIRET.K_M = :2 AND EJ = :5
UNION
                          SELECT omadaepi ,  'EP',  EJ
                          FROM  OMADAEPI
                          WHERE ( omadaepi.kat = :3 OR omadaepi.kat = '00' )  AND  omadaepi.ak = :4  AND
                                        omadaepi.K_M = :2 AND EJ = :5  "
